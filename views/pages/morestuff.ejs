<body>
    <center>
        <hugeText>
            random audio i found
        </hugeText>
        </br>
        <audio controls>
            <source src="/audio/woosh.mp3/" type="audio/mpeg"/>
            Unsupported browser
        </audio>
        <script>
            var c = 0
            function spamATON(){
                c += 1
                document.getElementsByName("button").forEach(element => {
                    element.textContent = `${c} spamtered`
                })
                const img = document.createElement("img")
                img.src = "/images/spamton.png"
                for (let i = 0; i < c; i++) {
                    document.getElementById("spamtom").appendChild(img)
                }
            }
        </script>
        </br>
        <button name="button" onclick="spamATON()">
            click me
        </button>
        <div id="spamtom"></div>
    </center>
</body>